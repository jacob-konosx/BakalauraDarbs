# WebODM konfigurācija
odm_url = "http://localhost:8000"
odm_username = "admin"
odm_password = "supersecret"

# Sensoru datu API pamata URL
sensoru_datu_url = "https://example.com/api/sensors"

# Sīkdatņu šifrēšanas parole
sikdatnes_parole = "random_generated_encryption_key"

[postgres_db]
# PostgreSQL savienojuma parametri
port = 5432
host = "localhost"
datu_baze = "gis_db"
lietotajs = "postgres"
parole = "your_db_password"

[auth]
# Google OAuth 2.0 autentifikācijas parametri
client_id = "your_google_client_id.apps.googleusercontent.com"
client_secret = "your_google_client_secret"
cookie_secret = "long_random_string_for_cookie_encryption"
redirect_uri = "http://localhost:8501/oauth2callback"
server_metadata_url = "https://accounts.google.com/.well-known/openid-configuration"

[server]
# Maksimālais augšupielādes izmērs megabaitos
maxUploadSize = 400
